#include <SFML/Graphics.h>
#include <stdlib.h>
#include <stdio.h>
#include "my.h"
#include "my_runner.h"

int init_character(objects_t *objects)
{
	parallax->m3_t = sfTexture_createFromFile(M3_PATH, NULL);
	if (!parallax->m3_t)
		return (1);
	parallax->m3_s = sfSprite_create();
	sfSprite_setTexture(parallax->m3_s, parallax->m3_t, sfTrue);
	parallax->m3_p.x = 0;
	parallax->m3_p.y = 460;
	sfSprite_setPosition(parallax->m3_s, parallax->m3_p);
	parallax->m3_m.x = -6;
	return (0);
}
